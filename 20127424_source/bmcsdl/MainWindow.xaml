<Window x:Class="bmcsdl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bmcsdl"
        xmlns:uc="clr-namespace:bmcsdl.ResourceUserControl"
        Name="windowLogin"
        mc:Ignorable="d"
        Height="500" Width="850">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Button></Button>
        </Grid>
        <Grid Name="login" Grid.Row="1">
            <uc:loginUC></uc:loginUC>
        </Grid>
        <Grid Grid.Row="2" Name="quanlilophoc" Visibility="Collapsed">
            <uc:QuanLiLopUC></uc:QuanLiLopUC>
        </Grid>
    </Grid>
</Window>
